# Environment Configuration Template
# Copy this file to .env and replace with your actual values
# NEVER commit the .env file to version control

# Environment
NODE_ENV=production

# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key

# Google Cloud Storage Configuration (Required for GCS)
VITE_GCS_BUCKET_NAME=lex-legal-documents-bucket
VITE_GOOGLE_CLOUD_API_KEY=your_gcs_api_key

# AI Configuration (Required for AI features)
VITE_GEMINI_API_KEY=your_gemini_api_key

# Court Listener API (Required for document ingestion)
VITE_COURT_LISTENER_API_KEY=your_courtlistener_api_key

# Service URLs (Auto-detected in production)
VITE_PROXY_SERVER_URL=https://juristinsight.com
VITE_ELASTICSEARCH_URL=https://juristinsight.com/es

# Elasticsearch Authentication (Optional)
VITE_ELASTICSEARCH_USERNAME=
VITE_ELASTICSEARCH_PASSWORD=
